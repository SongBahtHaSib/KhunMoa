{
  "version": 2,
  "builds": [
    {
      "src": "wsgi_app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi_app.py"
    }
  ],
  "installCommand": "pip install python==3.9 && pip install -r requirements.txt", 
  "buildCommand": "echo 'Build complete'"
}
