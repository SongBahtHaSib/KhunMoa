{
  "version": 2,
  "builds": [
    {
      "src": "wsgi_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi_app.py"
    }
  ],
  "functions": {
    "wsgi_app.py": {
      "runtime": "python3.9"  // <--- กำหนดเวอร์ชัน Python ที่นี่
    }
  }
}
